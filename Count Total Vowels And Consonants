using System;
using System.Collections.Generic;

class Program
{
    public static void CountTotalVowelsAndConsonants(string s)
    {
        char[] ch = s.ToCharArray();
        int vowel = 0;
        int Constent = 0;
        for(int i = 0;i<ch.Length;i++)
        {
            if(ch[i]=='a'||ch[i]=='e'||ch[i]=='i'||ch[i]=='o'||ch[i]=='u')
            {
                vowel++;
            }
            else
            {
                Constent++;
            }
        }
        
        Console.WriteLine("Total Vowels in String is : "+vowel);
        Console.WriteLine("Total Constent in string is : "+Constent);
    }
    public static void Main(string[]args)
    {
        Console.WriteLine("Enter String : ");
        string s = Console.ReadLine();
        
        CountTotalVowelsAndConsonants(s);
    }
}
